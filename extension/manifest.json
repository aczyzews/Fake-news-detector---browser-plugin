{
  "name": "Fake news detector",
  "action": {},
  "manifest_version": 3,
  "version": "1.1.1",
  "description": "Tells if the currently shown webpage is fake or not.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "https://*/"
  ]
}